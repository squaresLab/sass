where
rewrite :[num] { "11" -> "12" },
rewrite :[num] { "10" -> "11" },
rewrite :[num] { "9" -> "10" },
rewrite :[num] { "8" -> "9" },
rewrite :[num] { "7" -> "8" },
rewrite :[num] { "6" -> "7" },
rewrite :[num] { "5" -> "6" },
rewrite :[num] { "4" -> "5" },
rewrite :[num] { "3" -> "4" },
rewrite :[num] { "2" -> "3" }
